{
  "urls": "http://*:80;",
  "serilog": {
    "minimumLevel": {
      "default": "Information"
    },
    "writeTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "RollingFile",
              "Args": { "pathFormat": "logs/donut/donut-docker-{Date}.log" }
            }
          ]
        }
      }
    ]
  },
  "AssetAccountServiceHost": "http://terminal-matching-engine"
}